<h1><strong>Тест</strong></h1>

<table border="11">
	<tr align="center">
		<th><strong>Title:</strong></th>
		<th><strong>Level:</strong></th>
		<th colspan='2'><strong>Actions</strong></th>
	</tr>
	<tr align="center">
		<td><%= @test.title %></td>
		<td><%= @test.level %></td>
		<td><%= button_to 'Edit', edit_test_path(@test), :method => :get %></td>
		<td><%= button_to 'Back', tests_path, :method => :get %></td>
</table>

<h1><strong>Вопросы к тесту</strong></h1>
<%= render @test.questions %>
<br>

