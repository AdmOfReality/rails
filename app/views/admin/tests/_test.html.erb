<tr align="center">
	<td><%= test.title %></td>
	<td><%= test_level(test) %></td>
	<td><%= test.category.title %></td>
	<td><%= test.owner.email %></td>
	<td><%= test.questions.count %></td>
	<td><%= button_to 'Show', admin_test_path(test), :method => :get %></td>
	<td><%= button_to 'Edit', edit_admin_test_path(test), :method => :get %></td>
	<td>
		<%= button_to 'Delete', admin_test_path(test),
								:method => :delete,
								data: {turbo_confirm: 'Are you sure O_o ???'} %>
	</td>
</tr>
