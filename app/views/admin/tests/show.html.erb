<h1><strong><%=t('tests.index.title')%></strong></h1>

<table border="11">
	<tr align="center">
		<th><strong><%=t('tests.index.title')%>:</strong></th>
		<th><strong><%=t('tests.index.level')%>:</strong></th>
		<th colspan='2'><strong><%=t('tests.index.actions')%></strong></th>
	</tr>
	<tr align="center">
		<td><%= @test.title %></td>
		<td><%= @test.level %></td>
		<td><%= button_to t('global_buttons.edit'), edit_admin_test_path(@test), :method => :get %></td>
		<td><%= button_to t('global_buttons.back'), admin_tests_path, :method => :get %></td>
</table>


<h1><strong><%= t('.questions')%></strong></h1>
<ol>
<%= render partial: 'questions/question', collection: @test.questions %>
</ol>

